[
  {
    "judul": "Nadzom Aqidatul Awam",
    "isi": [
      "بَدَأْتُ بِسْمِ اللّٰهِ وَالرَّحْمٰنِ",
      "رَحِيْمٍ وَهُوَ الرَّحْمٰنُ",
      "الحَمْدُ لِلّٰهِ الَّذِي تَقَدَّسَا"
    ]
  },
  {
    "judul": "Nadzom Sifat 20",
    "isi": [
      "وُجُوْدُ اللهِ وَاجِبٌ لِذَاتِهِ",
      "كَذَا قِدَمْ دَوْمٌ كَذَا مُخَالَفَتُهُ",
      "وَقِيَامُهُ بِذَاتِهِ وَالْوَحْدَانِيَّةُ"
    ]
  }
]
