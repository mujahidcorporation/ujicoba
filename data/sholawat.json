[
  {
    "judul": "Sholawat Nariyah",
    "isi": [
      "اللَّهُمَّ صَلِّ صَلَاةً كَامِلَةً",
      "وَسَلِّمْ سَلَامًا تَامًّا",
      "عَلَى سَيِّدِنَا مُحَمَّدٍ",
      "الَّذِي تَنْحَلُّ بِهِ الْعُقَدُ"
    ]
  },
  {
    "judul": "Sholawat Munjiyat",
    "isi": [
      "اللَّهُمَّ صَلِّ عَلَى سَيِّدِنَا مُحَمَّدٍ",
      "صَلَاةً تُنْجِينَا بِهَا مِنْ جَمِيعِ الْأَهْوَالِ",
      "وَالْآفَاتِ وَتَقْضِي لَنَا بِهَا جَمِيعَ الْحَاجَاتِ"
    ]
  }
]
